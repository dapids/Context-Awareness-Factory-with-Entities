  <owl:ObjectProperty rdf:about="http://caffe.ns/home#holds">
    <rdfs:domain rdf:resource="http://caffe.ns/home#Person"/>
    <rdfs:range rdf:resource="http://caffe.ns/home#PassiveObj"/>
  </owl:ObjectProperty>
  <owl:Class rdf:about="http://caffe.ns/home#Guest">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Person"/>
  </owl:Class>
  <owl:Class rdf:about="http://caffe.ns/home#ActiveObj">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Obj"/>
  </owl:Class>
  <home:ActiveObj rdf:about="http://caffe.ns/home#television">
    <home:islocated>
      <home:Room rdf:about="http://caffe.ns/home#livingRoom">
        <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">livingRoom</home:ID>
      </home:Room>
    </home:islocated>
    <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">television</home:ID>
  </home:ActiveObj>
  <owl:Class rdf:about="http://caffe.ns/home#PassiveObj">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Obj"/>
  </owl:Class>
  <home:ActiveObj rdf:about="http://caffe.ns/home#toaster">
    <home:islocated>
      <home:Room rdf:about="http://caffe.ns/home#kitchen">
        <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">kitchen</home:ID>
      </home:Room>
    </home:islocated>
    <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">toaster</home:ID>
  </home:ActiveObj>
  <home:PassiveObj rdf:about="http://caffe.ns/home#spoon">
    <home:islocated rdf:resource="http://caffe.ns/home#livingRoom"/>
    <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">spoon</home:ID>
  </home:PassiveObj>
  <home:Guest rdf:about="http://caffe.ns/home#marco">
    <home:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marco_Sorrentino</home:name>
    <home:talksto>
      <home:Guest rdf:about="http://caffe.ns/home#andrea">
        <home:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Andrea_Monacchi</home:name>
        <home:talksto rdf:resource="http://caffe.ns/home#marco"/>
        <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">andrea</home:ID>
      </home:Guest>
    </home:talksto>
    <home:talksto>
      <home:Host rdf:about="http://caffe.ns/home#david">
        <home:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David_Sorrentino</home:name>
        <home:talksto rdf:resource="http://caffe.ns/home#marco"/>
        <home:uses rdf:resource="http://caffe.ns/home#toaster"/>
        <home:uses rdf:resource="http://caffe.ns/home#television"/>
        <home:holds rdf:resource="http://caffe.ns/home#bowl"/>
        <home:holds rdf:resource="http://caffe.ns/home#spoon"/>
        <home:graduated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">True</home:graduated>
        <home:age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">27</home:age>
        <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">david</home:ID>
        <home:islocated rdf:resource="http://caffe.ns/home#livingRoom"/>
      </home:Host>
    </home:talksto>
    <home:age rdf:datatype="http://www.w3.org/2001/XMLSchema#int">26</home:age>
    <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">marco</home:ID>
    <home:islocated rdf:resource="http://caffe.ns/home#livingRoom"/>
  </home:Guest>
  <home:PassiveObj rdf:about="http://caffe.ns/home#bowl">
    <home:islocated rdf:resource="http://caffe.ns/home#livingRoom"/>
    <home:ID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bowl</home:ID>
  </home:PassiveObj>
  <owl:Class rdf:about="http://caffe.ns/home#Room"/>
  <owl:Class rdf:about="http://caffe.ns/home#Obj">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://caffe.ns/home#Thing"/>
  <owl:Class rdf:about="http://caffe.ns/home#Host">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Person"/>
  </owl:Class>
  <owl:Class rdf:about="http://caffe.ns/home#Person">
    <rdfs:subClassOf rdf:resource="http://caffe.ns/home#Thing"/>
  </owl:Class>
