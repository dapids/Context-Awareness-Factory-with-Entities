  xmlns:home="http://caffe.ns/home#"
>
  <owl:DatatypeProperty rdf:about="http://caffe.ns/home#graduated">
    <rdfs:domain rdf:resource="http://caffe.ns/home#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://caffe.ns/home#talksto">
    <rdfs:domain rdf:resource="http://caffe.ns/home#Person"/>
    <rdfs:range rdf:resource="http://caffe.ns/home#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://caffe.ns/home#islocated">
    <rdfs:domain rdf:resource="http://caffe.ns/home#Thing"/>
    <rdfs:range rdf:resource="http://caffe.ns/home#Room"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://caffe.ns/home#name">
    <rdfs:domain rdf:resource="http://caffe.ns/home#Person"/>
